          /**
          //style it?
          [draggable="true"] {
              border-style: dashed dashed dashed solid !important;
              border-width: 2px 2px 2px 3px !important;
          
              border-t-color: #c1c7cf !important;
              border-b-color: #c1c7cf !important;
              border-r-color: #c1c7cf !important;
          }
          */
#}
        });
      </script>
